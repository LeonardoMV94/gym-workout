    <div class="header-bottom">
      <div class="container">
        <div class="social">
          <ul>
            <li class="facebook">
              <a href="https://web.facebook.com/WorkoutCentroDeportivo/?locale=es_LA&_rdc=1&_rdr">
                <span></span>
              </a>
            </li>

            <li class="instagram">
              <a href="https://www.instagram.com/workoutsanfelipe/">
                <span></span>
              </a>
            </li>

          </ul>
        </div>
        <div class="clear"></div>
      </div>
    </div>

    {% include "./partials/menu.liquid" %}

    <div class="main">
      <div class="register-grids">
        <div class="container">
          <form id="formRegistro">
            <div class="register-top-grid">
              <h3>INFORMACION PERSONAL</h3>
              <div>
                <span>Nombres<label>*</label>
                </span>
                <input
                  type="text"
                  id="nombre"
                  name="nombre"
                  pattern="^[a-zA-Z]*$"
                  title="Solo letras"
                  placeholder="Nombre" />
              </div>
              <div>
                <span>Apellido Paterno<label>*</label>
                </span>
                <input
                  type="text"
                  name="apellidoP"
                  pattern="^[a-zA-ZÀ-ÿ\u00f1\u00d1]+(\s*[a-zA-ZÀ-ÿ\u00f1\u00d1]*)*[a-zA-ZÀ-ÿ\u00f1\u00d1]+$"
                  title="Solo letras"
                  placeholder="Apellido Paterno">
              </div>
              <div>
                <span>Apellido Materno<label>*</label>
                </span>
                <input
                  type="text"
                  name="apellidoM"
                  pattern="^[a-zA-ZÀ-ÿ\u00f1\u00d1]+(\s*[a-zA-ZÀ-ÿ\u00f1\u00d1]*)*[a-zA-ZÀ-ÿ\u00f1\u00d1]+$"
                  title="Solo letras"
                  placeholder="Apellido Paterno">
              </div>
              <div>
                <span>Edad<label>*</label>
                </span>
                <input
                  type="number"
                  min="18"
                  max="120"
                  name="edad"
                  placeholder="edad">
              </div>
              <div>
                <span>Email<label>*</label>
                </span>
                <input
                  type="email"
                  pattern="[a-z0-9._%+-]+@[a-z0-9.-]+\.[a-z]{2,4}$"
                  name="correo"
                  title="Ingrese un correo valido"
                  placeholder="Email">
              </div>
              <div>
                <span>celular<label>*</label>
                </span>
                <input
                  type="text"
                  name="celular"
                  pattern="/\A(\+?56)?(\s?)(0?9)(\s?)[9876543]\d{7}\z/"
                  placeholder="celular">
              </div>
              <div class="clear"></div>
              <!--<a class="news-letter" href="#">
              <label class="checkbox"><input
              type="checkbox"
              name="checkbox"
              checked=""><i></i>¿Desea recibir nuevas ofertas?</label>
              </a>-->
              <div class="clear"></div>
            </div>
            <div class="clear"></div>
            <div class="register-bottom-grid">
              <h3>REGISTRO</h3>
              <div>
                <span>Contraseña<label>*</label>
                </span>
                <input
                  type="password"
                  name="password"
                  placeholder="Password">
              </div>
              <div>
                <span>Confirmar Contraseña<label>*</label>
                </span>
                <input
                  type="password"
                  name="passwordOther"
                  placeholder="Confirmar">
              </div>
              <div class="clear"></div>
            </div>
            <div class="clear"></div>
            <button type="submit" class="btn4">
              Registrate
            </button>
          </form>
        </div>
      </div>
    </div>
